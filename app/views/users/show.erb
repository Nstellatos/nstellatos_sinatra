<h2> <%= @user.name%>'s Show Page </h2>
<h3> Email: </h3>
<h4> <%= @user.email%> </h4>

 
<br><br>

<% current_user.positive_posts.each do |positive_post| %>
   <h4><a href="/positive_posts/<%= positive_post.id %>"> <%= positive_post.created_at %></a></h4>

  <p> <%= positive_post.content %> </p>
  <p>By <%= positive_post.user.name %></p>
    
<br><br>
<% end %>